<%- include("header.ejs") -%>

    <div class="box" id="heading">
            <h1><%= listsTitle %></h1>
    </div>
    

    <div class="box">
    

        <% for(let i=0; i<addNewItems.length; i++){ %>
        <div class="item">
            <input type="checkbox" >
            <p><%= addNewItems[i]%></p>

        </div> 
        <% } %>
    


        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off" autofocus>
            <button type="submit" name="list" value=<%= listsTitle%>>+</button>
        </form>    
        
    </div>
<%- include("footer.ejs") -%>